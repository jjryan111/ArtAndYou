@model IEnumerable<ArtAndYou.Models.Survey3>

@{
    ViewBag.Title = "Survey Page 3";
}
<form name="Medium" id="Choice1" action="@Url.Action("Show4Survey3", "SurveyFinal")" method="post">

    <table class="table">
        
        <tr>
        @foreach (var item in Model)
            {
                <td>
                    <input type="radio" name="Genre" value=@item.Genre>
                    <img src="@Url.Content(item.ImageUrl)" height="200" />
                </td>
        
            }
        </tr>
         </table>
    <input type="submit" id="ClassSubmit" value="Submit">
</form>
    </table>
