<script src="jquery-3.2.1.js"></script>
<script src="~/Scripts/THing.js"></script>

@{
    ViewBag.Title = "Classification Info";
}

<h2>Classification Info</h2>

@ViewBag.Classifications

<html>
<body>
    <p id="0">test</p>
    <script>
            var str = "@ViewBag.Classifications";
            var res = str.split(",");
            var i = 0;
            for (i = 0; i < 100; i++)
            {
                document.getElementById(0).innerHTML = res[0];
            }
    </script>
</body>
</html>