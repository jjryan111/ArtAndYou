<script src="jquery-3.2.1.js"></script>
<script src="~/Scripts/THing.js"></script>

@{
    ViewBag.Title = "Portfolio";
}

<h2>Portfolio</h2>

<html>
    <body>
        <img src="nothing.jpg" id="0" name="image" />
        <img src="nothing.jpg" id="1" name="image" />
        <img src="nothing.jpg" id="2" name="image" />
        <img src="nothing.jpg" id="3" name="image" />
        <img src="nothing.jpg" id="4" name="image" />
        <img src="nothing.jpg" id="5" name="image" />

        <script>
            var str = "@ViewBag.ObjectID";
            var res = str.split(",");
            var i = 0;
            for (i = 0; i < 6; i++)
            {
                document.getElementById(i).src = res[i];
            }
        </script>
    </body>
</html>

