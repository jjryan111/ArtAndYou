<script src="jquery-3.2.1.js"></script>
<script src="~/Scripts/THing.js"></script>

@{
    ViewBag.Title = "Portfolio";
}

<h1>Selections for You.</h1>

<h2>This query returned <ins id="totalrecords"></ins> items.</h2>

<html>
    
    <body>
        <img src="nothing.jpg" id="1" name="image" alt=""/>
        <img src="nothing.jpg" id="2" name="image" alt=""/>
        <img src="nothing.jpg" id="3" name="image" alt=""/>
        <img src="nothing.jpg" id="4" name="image" alt=""/>
        <img src="nothing.jpg" id="5" name="image" alt=""/>
        <img src="nothing.jpg" id="6" name="image" alt=""/>
        <script>
            var str = "@ViewBag.ObjectID";
            var res = str.split(",");
            var i = 0;
            document.getElementById("totalrecords").innerHTML = res[0];
            for (i = 1; i < 7; i++)
            {
                document.getElementById(i).src = res[i];
            }
        </script>
    </body>
</html>